      "\n{",
      "  type: 'UnorderList',",
      "  depth: $1,",
      "  body: [$1],",
      "  isClosed: $2,",
      "},$3"
    ]
  },
  "OrderList": {
    "prefix": "nol",
    "body": [
      "\n{",
      "  type: 'OrderList',",
      "  start: '$1',",
      "  styleType: '1',",
      "  depth: $1,",
      "  body: [$2],",
      "  isClosed: $3,",
      "},$4"
    ]
  },
  "ListItem": {
    "prefix": "nli",
