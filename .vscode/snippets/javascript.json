/* need extension https://marketplace.visualstudio.com/items?itemName=rebornix.project-snippets */
{
  "Chars": {
    "prefix": "tc",
    "body": [
      "\n{",
      "  type: 'Chars',",
      "  value: '$1',",
      "  start: $2,",
      "  end: $3,",
      "},$4"
    ],
    "description": "Token has got type Chars"
  },
  "Signs": {
    "prefix": "ts",
    "body": [
      "\n{",
      "  type: 'Signs',",
      "  value: '=',",
      "  amount: $1,",
      "  start: $2,",
      "  end: $3,",
      "},$4"
    ]
  },
  "Hyphens": {
    "prefix": "thy",
    "body": [
      "\n{",
      "  type: 'Hyphens',",
      "  value: '-',",
      "  amount: $1,",
      "  start: $2,",
      "  end: $3,",
      "},$4"
    ]
  },
  "Hashes": {
    "prefix": "th",
    "body": [
      "\n{",
      "  type: 'Hashes',",
      "  value: '#',",
      "  amount: $1,",
      "  start: $2,",
      "  end: $3,",
      "},$4"
    ]
  },
  "Bullet": {
    "prefix": "tb",
    "body": [
      "\n{",
      "  type: 'Bullet',",
      "  depth: $1,",
      "  value: '$2',",
      "  start: $3,",
      "  end: $4,",
      "},$5"
    ],
    "description": "Token has got type Chars"
  },
  "Item": {
    "prefix": "ti",
    "body": [
      "\n{",
      "  type: 'Item',",
      "  depth: $1,",
      "  value: $2,",
      "  start: $3,",
      "  end: $4,",
      "},$5"
    ],
    "description": "Token has got type Chars"
  },
  "NewLine": {
    "prefix": "tnl",
    "body": [
      "\n{",
      "  type: 'NewLine',",
      "  amount: $1,",
      "  value: '$2',",
      "  start: $3,",
      "  end: $4,",
      "},$5"
    ],
    "description": "Token has got type NewLine"
  },
  "Asterisk": {
    "prefix": "ta",
    "body": [
      "\n{",
      "  type: 'Asterisk',",
      "  amount: $1",
      "  value: '*'",
      "  start: $2",
      "  end: $3",
      "},$4"
    ],
    "description": "Token has got type Asterisk"
  },
  "Underscore": {
    "prefix": "tu",
    "body": [
      "\n{",
      "  type: 'Underscore',",
      "  amount: $1",
      "  value: '_'",
      "  start: $2",
      "  end: $3",
      "},$4"
    ],
    "description": "Token has got type Underscore"
  },
  "Program": {
    "prefix": "ast",
    "body": [
      "\n{",
      "  type: 'Program',",
      "  body: [$1],",
      "},$2"
    ],
    "description": "Node has got type Program"
  },
  "Paragraph": {
    "prefix": "np",
    "body": [
      "\n{",
      "  type: 'Paragraph',",
      "  body: [$1],",
      "  isClosed: $2,",
      "}$3"
    ],
    "description": "Node has got type Paragraph"
  },
  "UnorderList": {
    "prefix": "nul",
    "body": [
      "\n{",
      "  type: 'UnorderList',",
      "  depth: $1,",
      "  body: [$1],",
      "  isClosed: $2,",
      "},$3"
    ],
    "description": "Node has got type UnorderList"
  },
  "OrderList": {
    "prefix": "nol",
    "body": [
      "\n{",
      "  type: 'OrderList',",
      "  start: '$1',",
      "  styleType: '1',",
      "  depth: $1,",
      "  body: [$2],",
      "  isClosed: $3,",
      "},$4"
    ],
    "description": "Node has got type OrderList"
  },
  "ListItem": {
    "prefix": "nli",
    "body": [
      "\n{",
      "  type: 'ListItem',",
      "  value: '$1',",
      "  body: [$2],",
      "  isClosed: $3,",
      "},$4"
    ],
    "description": "Node has got type ListItem"
  },
  "Header": {
    "prefix": "nh",
    "body": [
      "\n{",
      "    type: 'Header',",
      "    styleType: '1',",
      "    value: '$1',",
      "    body: [$2],",
      "    isClosed: $4,",
      "},$5"
    ]
  },
  "CharsNode": {
    "prefix": "nc",
    "body": [
      "\n{",
      "  type: 'Chars',",
      "  value: '$1'",
      "}$2"
    ],
    "description": "Node has got type Chars"
  },
  "Italic": {
    "prefix": "ni",
    "body": [
      "\n{",
      "  type: 'Italic',",
      "  operator: '$1',",
      "  body: [$2],",
      "  isClosed: $3,",
      "}$4"
    ],
    "description": "Node has got type Italic"
  },
  "Bold": {
    "prefix": "nb",
    "body": [
      "\n{",
      "  type: 'Bold',",
      "  operator: '$1',",
      "  body: [$2],",
      "  isClosed: $3,",
      "}$4"
    ],
    "description": "Node has got type Bold"
  },
  "EOF": {
    "prefix": "eof",
    "body": [
      "\n{",
      "  type: 'EOF',",
      "},"
    ]
  },
  "BOF": {
    "prefix": "bof",
    "body": [
      "\n{",
      "  type: 'BOF',",
      "},"
    ]
  }
}
