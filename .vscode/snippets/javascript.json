/* need extension https://marketplace.visualstudio.com/items?itemName=rebornix.project-snippets */
{
  "Backtick": {
    "prefix": "tcb",
    "body": [
      "\n{",
      "  type: 'CodeBlock',",
      "  value: '$1',",
      "  isClosed: $2,",
      "  start: $3,",
      "  end: $4,",
      "}$5"
    ]
  },
  "Chars": {
    "prefix": "tc",
    "body": [
      "\n{",
      "  type: 'Chars',",
      "  value: '$1',",
      "  start: $2,",
      "  end: $3,",
      "},$4"
    ]
  },
  "Signs": {
    "prefix": "ts",
    "body": [
      "\n{",
      "  type: 'Signs',",
      "  amount: $1,",
      "  value: '=',",
      "  start: $2,",
      "  end: $3,",
      "},$4"
    ]
  },
  "Hyphens": {
    "prefix": "thy",
    "body": [
      "\n{",
      "  type: 'Hyphens',",
      "  amount: $1,",
      "  value: '-',",
      "  start: $2,",
      "  end: $3,",
      "},$4"
    ]
  },
  "Hashes": {
    "prefix": "th",
    "body": [
      "\n{",
      "  type: 'Hashes',",
      "  amount: $1,",
      "  value: '#',",
      "  start: $2,",
      "  end: $3,",
      "},$4"
    ]
  },
  "Bullet": {
    "prefix": "tb",
    "body": [
      "\n{",
      "  type: 'Bullet',",
      "  depth: $1,",
      "  value: '$2',",
      "  start: $3,",
      "  end: $4,",
      "},$5"
    ]
  },
  "Item": {
    "prefix": "ti",
    "body": [
      "\n{",
      "  type: 'Item',",
      "  depth: $1,",
      "  value: $2,",
      "  start: $3,",
      "  end: $4,",
      "},$5"
    ]
  },
  "NewLine": {
    "prefix": "tnl",
    "body": [
      "\n{",
      "  type: 'NewLine',",
      "  amount: $1,",
      "  value: '$2',",
      "  start: $3,",
      "  end: $4,",
      "},$5"
    ]
  },
  "Asterisk": {
    "prefix": "ta",
    "body": [
      "\n{",
      "  type: 'Asterisk',",
      "  amount: $1,",
      "  value: '*',",
      "  start: $2,",
      "  end: $3,",
      "},$4"
    ]
  },
  "Underscore": {
    "prefix": "tu",
    "body": [
      "\n{",
      "  type: 'Underscore',",
      "  amount: $1,",
      "  value: '_',",
      "  start: $2,",
      "  end: $3,",
      "},$4"
    ]
  },
  "CodeInline": {
    "prefix": "tci",
    "body": [
      "\n{",
      "  type: 'Code',",
      "  value: '$1',",
      "  isClosed: $2,",
      "  start: $3,",
      "  end: $4,",
      "}$5"
    ]
  },
  "Program": {
    "prefix": "ast",
    "body": [
      "{",
      "  type: 'Program',",
      "  body: [$1],",
      "  parent: null,",
      "},$2"
    ]
  },
  "Paragraph": {
    "prefix": "np",
    "body": [
      "\n{",
      "  type: 'Paragraph',",
      "  body: [$1],",
      "  isClosed: $2,",
      "}$3"
    ]
  },
  "UnorderList": {
    "prefix": "nul",
